  "runtime_flows": [
    {
      "name": "Create meeting",
      "steps": [
        "UI: MeetingControls -> useMeetingState.startMeeting",
        "Frontend API: POST /api/meetings",
        "Backend API: api/meetings.create_meeting",
        "Service: MeetingService.create",
        "Repository: MeetingRepository.create",
        "DB: insert into meetings"
      ]
    },
    {
      "name": "Stop meeting",
      "steps": [
        "UI: MeetingControls.stop",
        "Frontend API: POST /api/meetings/{id}/stop",
        "Backend API -> Service.stop -> Repository.stop",
        "DB: update meetings set status='stopped', ended_at"
      ]
    },
    {
      "name": "Create ephemeral token",
      "steps": [
        "Frontend: POST /api/token/ephemeral",
        "Backend API: api/token.create_ephemeral_token",
        "Provider: OpenAIRealtimeProvider or ElevenLabsRealtimeProvider",
        "Response: provider + client_secret + expires_at"
      ]
    },
    {
      "name": "Realtime delta update",
      "steps": [
        "Client WS sends transcript.segment",
        "Backend WS -> StateManager.process_transcript_segment",
        "StateManager обновляет summary/insights + dedup",
        "WebSocketConnectionManager broadcast meeting.delta",
        "Frontend LiveMeeting рендерит SummaryPanel и InsightsPanel"
      ]
    },
    {
      "name": "Copilot Q&A",
      "steps": [
        "UI: CopilotChat sends user.question over WS",
        "Backend WS: state_manager.answer_question",
        "Backend WS: broadcast bot.answer",
        "UI: LiveMeeting renders bot answer in chat"
      ]
    },
    {
      "name": "Post-meeting export",
      "steps": [
        "UI: PostMeetingReport -> ExportButtons",
        "Frontend API: GET /api/meetings/{id}/export?format=md|html",
        "Backend API: meetings.export_meeting",
        "Service: ExportService.render_markdown/render_html",
        "Optional: POST /api/meetings/{id}/export/telegram"
      ]
    }
  ],
